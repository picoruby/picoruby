class SSLSocket < BasicSocket
  def initialize: (SSLContext ssl_context) -> void
  def self.open: (TCPSocket tcp_socket, SSLContext ssl_context) -> SSLSocket
  def hostname=: (String hostname) -> String
  def connect: () -> self
  def addr: () -> Array[String | Integer]
  def connected?: () -> bool
  def peer_cert: () -> nil
  def peer_cert_chain: () -> Array[untyped]
  def cipher: () -> nil
  def ssl_version: () -> String
  def state: () -> String
end
